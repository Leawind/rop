	"version": "0.0.1",
	"license": "MIT",
	"homepage": "https://github.com/Leawind/rop",
	"bugs": "https://github.com/Leawind/rop/issues",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Leawind/rop.git"
	},
	"author": "Leawind <leawind@yeah.net>",
	"type": "module",
	"keywords": [
		"typescript",
		"javascript",
